<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox prefWidth="696.0" spacing="10.0" styleClass="white-background" stylesheets="@../styles.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.song_finder_fx.UIControllerSearch">
   <Label text="Search Song Details">
      <font>
         <Font name="Segoe UI Semibold" size="28.0" />
      </font>
      <VBox.margin>
         <Insets left="30.0" top="30.0" />
      </VBox.margin>
   </Label>
   <Label layoutX="41.0" layoutY="115.0" text="Audios">
      <VBox.margin>
         <Insets left="31.0" top="30.0" />
      </VBox.margin>
      <font>
         <Font name="Segoe UI Semibold" size="12.0" />
      </font>
   </Label>
   <VBox fx:id="textAreaVbox" maxWidth="606.0" minWidth="600.0" prefWidth="606.0" styleClass="textarea-bg" stylesheets="@../styles.css">
      <VBox.margin>
         <Insets left="30.0" right="30.0" />
      </VBox.margin>
      <TextField fx:id="searchArea" onKeyPressed="#getText" promptText="Enter song name">
         <VBox.margin>
            <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
         </VBox.margin>
      </TextField>
      <VBox fx:id="vboxSong">
         <VBox.margin>
            <Insets bottom="30.0" left="30.0" right="30.0"/>
         </VBox.margin>
         <Label fx:id="searchResult" text="Search"/>
         <HBox prefHeight="50.0">
            <ImageView fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true"/>
            <VBox>
               <HBox.margin>
                  <Insets left="20.0" top="5.0"/>
               </HBox.margin>
               <Label fx:id="srchRsSongName" text="Song Name">
                  <font>
                     <Font size="14.0"/>
                  </font>
               </Label>
               <Label fx:id="srchRsISRC" text="ISRC">
                  <VBox.margin>
                     <Insets top="5.0"/>
                  </VBox.margin>
               </Label>
            </VBox>
         </HBox>
      </VBox>
   </VBox>
   <HBox fx:id="searchAndCollect" alignment="CENTER_RIGHT" maxWidth="606.0" minHeight="67.0" minWidth="600.0" prefHeight="67.0" prefWidth="485.0" styleClass="textarea-bg" stylesheets="@../styles.css">
      <VBox.margin>
         <Insets left="30.0" right="30.0" />
      </VBox.margin>
      <padding>
         <Insets left="20.0" />
      </padding>
      <ImageView fitHeight="90.0" fitWidth="23.0" opacity="0.8" pickOnBounds="true" preserveRatio="true">
         <Image url="@../images/icon%20_document%20download_.png" />
      </ImageView>
      <VBox alignment="CENTER_LEFT" prefHeight="74.0" prefWidth="350.0">
         <Label text="   Search and collect audios">
            <font>
               <Font name="Segoe UI Semilight" size="14.0" />
            </font>
            <VBox.margin>
               <Insets />
            </VBox.margin>
         </Label>
      </VBox>
      <ImageView fx:id="ProgressView" fitHeight="64.0" fitWidth="81.0" pickOnBounds="true" preserveRatio="true" visible="false">
         <HBox.margin>
            <Insets />
         </HBox.margin>
         <Image url="@../windows11-loading.gif" />
      </ImageView>
   </HBox>
</VBox>
