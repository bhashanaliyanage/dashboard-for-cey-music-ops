<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="mainVBox" prefWidth="696.0" stylesheets="@../styles.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <Label text="Song List">
        <font>
            <Font name="Segoe UI Semibold" size="28.0" />
        </font>
        <VBox.margin>
            <Insets left="30.0" top="30.0" />
        </VBox.margin>
    </Label>
    <VBox fx:id="vBoxInSearchSong" maxWidth="606.0" minWidth="600.0" prefWidth="606.0" styleClass="textarea-bg" stylesheets="@../styles.css">
        <VBox.margin>
            <Insets bottom="10.0" left="30.0" right="30.0" top="30.0" />
        </VBox.margin>
      <HBox alignment="CENTER_LEFT">
         <children>
            <VBox>
               <children>
                  <Label minWidth="60.0" text="Songs">
                     <font>
                        <Font name="Segoe UI Semibold" size="19.0" />
                     </font>
                  </Label>
                  <Label fx:id="lblListCount" text="Total: 00" />
               </children>
               <HBox.margin>
                  <Insets right="374.0" />
               </HBox.margin>
            </VBox>
            <Button fx:id="btnAddMore" minHeight="30.0" minWidth="110.0" onAction="#onAddMoreButtonClicked" styleClass="button_inside" stylesheets="@../styles.css" text="Add More Songs" />
         </children>
         <VBox.margin>
            <Insets left="30.0" right="30.0" top="20.0" />
         </VBox.margin>
      </HBox>
        <ScrollPane fx:id="scrlpneSong" hbarPolicy="NEVER" minViewportHeight="240.0" prefWidth="544.0" styleClass="scroll-pane" stylesheets="@../styles.css" vbarPolicy="ALWAYS" VBox.vgrow="ALWAYS">
            <VBox.margin>
                <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
            </VBox.margin>
            <VBox fx:id="vboxSong" prefWidth="525.0" styleClass="search-results-pane" stylesheets="@../styles.css" />
        </ScrollPane>
    </VBox>
    <VBox fx:id="hboxListActions" alignment="CENTER" maxWidth="606.0" minWidth="600.0" prefHeight="0.0" prefWidth="485.0" styleClass="textarea-bg" stylesheets="@../styles.css">
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
        </padding>
        <VBox.margin>
            <Insets bottom="30.0" left="30.0" />
        </VBox.margin>
        <Button fx:id="btnCopyTo" minHeight="30.0" minWidth="580.0" onAction="#onCopyToButtonClicked" styleClass="button_inside" stylesheets="@../styles.css" text="Copy List to Location">
         <VBox.margin>
            <Insets />
         </VBox.margin>
        </Button>
        <Button fx:id="btnGenerateInvoice" layoutX="31.0" layoutY="29.0" minHeight="30.0" minWidth="580.0" onAction="#onGenerateInvoiceButtonClicked" styleClass="button_inside" stylesheets="@../styles.css" text="Generate Invoice">
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin></Button>
    </VBox>
</VBox>
