<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="1000.0" prefWidth="750.0" stylesheets="@../../styles.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.song_finder_fx.ControllerIngestView">
    <HBox>
        <VBox.margin>
            <Insets left="30.0" right="30.0" top="30.0" />
        </VBox.margin>
      <HBox minWidth="370.0" spacing="10.0">
         <children>
              <Label onMouseClicked="#onGoBack" text="Ingests" textFill="#656565">
                 <font>
                     <Font name="Segoe UI Semibold" size="28.0" />
                 </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
              </Label>
            <Label text="&gt; Ingests to Approve" textFill="#656565">
               <font>
                  <Font name="Segoe UI Semibold" size="28.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <Label fx:id="lblIngestName" layoutX="111.0" layoutY="10.0" text="&gt; Ingest Name">
         <font>
            <Font name="Segoe UI Semibold" size="28.0" />
         </font>
         <HBox.margin>
            <Insets left="10.0" />
         </HBox.margin>
      </Label>
    </HBox>
   <HBox alignment="CENTER_LEFT" prefWidth="200.0" spacing="10.0">
      <children>
         <HBox spacing="5.0" HBox.hgrow="ALWAYS">
            <children>
               <Button mnemonicParsing="false" onAction="#onAssignPayees" prefHeight="30.0" prefWidth="165.0" styleClass="button_inside" text="Assign Payees" />
            </children>
         </HBox>
         <HBox alignment="CENTER_RIGHT" spacing="5.0" HBox.hgrow="ALWAYS">
            <children>
               <Button disable="true" mnemonicParsing="false" onAction="#onApproveIngest" prefHeight="30.0" prefWidth="165.0" styleClass="button_inside_brown" text="Approve Ingest" />
               <Button disable="true" mnemonicParsing="false" onAction="#onSaveEdits" prefHeight="30.0" prefWidth="165.0" styleClass="button_inside_brown" text="Save Edits" />
            </children>
            <HBox.margin>
               <Insets />
            </HBox.margin>
         </HBox>
      </children>
      <VBox.margin>
         <Insets left="30.0" right="30.0" top="30.0" />
      </VBox.margin>
   </HBox>
   <VBox maxWidth="688.0" minHeight="-Infinity" prefWidth="688.0" spacing="5.0" styleClass="textarea-bg" stylesheets="@../../styles.css" VBox.vgrow="ALWAYS">
      <VBox.margin>
         <Insets bottom="10.0" left="30.0" top="10.0" />
      </VBox.margin>
      <children>
         <TableView fx:id="tableIngest" prefWidth="200.0" VBox.vgrow="ALWAYS">
           <columns>
             <TableColumn prefWidth="75.0" text="C1" />
             <TableColumn prefWidth="75.0" text="C2" />
           </columns>
         </TableView>
      </children>
   </VBox>
</VBox>
