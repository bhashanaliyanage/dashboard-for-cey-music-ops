<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <Label text="Previous Claims">
      <font>
         <Font name="Segoe UI Semibold" size="19.0" />
      </font>
      <VBox.margin>
         <Insets left="20.0" top="20.0" />
      </VBox.margin>
   </Label>
   <HBox>
      <VBox.margin>
         <Insets left="20.0" right="20.0"/>
      </VBox.margin>
      <Label fx:id="lblClaimCount" text="12" textFill="#656565"/>
      <Label layoutX="10.0" layoutY="10.0" text="Manual Claims for" textFill="#656565">
         <HBox.margin>
            <Insets left="5.0"/>
         </HBox.margin>
      </Label>
      <Label fx:id="lblYouTubeID" layoutX="28.0" layoutY="10.0" text="youtube_id" textFill="#2e6070" underline="true">
         <HBox.margin>
            <Insets left="5.0"/>
         </HBox.margin>
         <cursor>
            <Cursor fx:constant="HAND"/>
         </cursor>
      </Label>
   </HBox>
   <ScrollPane fx:id="scrollPaneClaims">
      <VBox.margin>
         <Insets left="20.0" right="20.0" top="30.0"/>
      </VBox.margin>
      <VBox fx:id="vBoxClaims" prefWidth="250.0" spacing="10.0"/>
   </ScrollPane>
</VBox>
