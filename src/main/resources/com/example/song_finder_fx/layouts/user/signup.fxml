<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox maxWidth="688.0" prefWidth="688.0" stylesheets="@../../styles.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox>
         <VBox.margin>
            <Insets left="20.0" right="20.0" top="20.0" />
         </VBox.margin>
         <children>
            <Label text="Sign Up">
               <font>
                  <Font name="Segoe UI Semibold" size="19.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="DEFAULT" />
               </cursor>
            </Label>
            <Label text="(Create a new account)" textFill="#656565">
               <cursor>
                  <Cursor fx:constant="DEFAULT" />
               </cursor>
            </Label>
         </children>
      </VBox>
      <HBox spacing="10.0">
         <VBox.margin>
            <Insets left="20.0" right="20.0" top="30.0" />
         </VBox.margin>
         <children>
            <TextField fx:id="txtUsername1" onAction="#urlOnAction" prefHeight="30.0" promptText="Username" styleClass="textarea-bg" HBox.hgrow="ALWAYS" />
            <TextField fx:id="txtPassword1" onAction="#urlOnAction" prefHeight="30.0" promptText="Nickname" styleClass="textarea-bg" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <HBox layoutX="31.0" layoutY="106.0" spacing="10.0">
         <children>
            <TextField fx:id="txtUsername11" onAction="#urlOnAction" prefHeight="30.0" promptText="Email" styleClass="textarea-bg" HBox.hgrow="ALWAYS" />
            <TextField fx:id="txtPassword11" onAction="#urlOnAction" prefHeight="30.0" promptText="Password" styleClass="textarea-bg" HBox.hgrow="ALWAYS" />
         </children>
         <VBox.margin>
            <Insets left="20.0" right="20.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <Button fx:id="btnAddClaim1" mnemonicParsing="false" onAction="#onAddManualClaim" prefHeight="30.0" prefWidth="687.0" styleClass="button_inside_brown" text="Sign Up">
         <VBox.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="30.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
